{
	"name": "developion/craft-field-parser",
	"description": "Field value parser for non-craftcms utilities and services.",
	"type": "craft-plugin",
	"license": "mit",
	"version": "1.0.1",
	"support": {
		"email": "marko.gajic@developion.com",
		"issues": "https://github.com/developion/craft-field-parser/issues?state=open",
		"source": "https://github.com/developion/craft-field-parser",
		"docs": "https://github.com/developion/craft-field-parser",
		"rss": "https://github.com/developion/craft-field-parser/releases.atom"
	},
	"require": {
		"php": ">=8.0.2",
		"craftcms/cms": "^4.4"
	},
	"require-dev": {
		"craftcms/ecs": "dev-main",
		"craftcms/phpstan": "dev-main"
	},
	"autoload": {
		"psr-4": {
			"Developion\\FieldParser\\": "src/"
		}
	},
	"extra": {
		"handle": "field-parser",
		"name": "Field Parser",
		"developer": "Developion",
		"documentationUrl": "https://github.com/developion/craft-field-parser"
	},
	"scripts": {
		"check-cs": "ecs check --ansi",
		"fix-cs": "ecs check --ansi --fix",
		"phpstan": "phpstan --memory-limit=1G"
	},
	"config": {
		"sort-packages": true,
		"platform": {
			"php": "8.0.2"
		},
		"allow-plugins": {
			"yiisoft/yii2-composer": true,
			"craftcms/plugin-installer": true
		}
	}
}
